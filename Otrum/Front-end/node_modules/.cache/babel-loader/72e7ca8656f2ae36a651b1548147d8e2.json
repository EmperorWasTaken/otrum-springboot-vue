{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PersonServices from '../services/PersonServices';\nexport default {\n  name: 'PersonsList',\n\n  data() {\n    return {\n      personsList: [],\n      personName: '',\n      personAge: ''\n    };\n  },\n\n  methods: {\n    getPersons() {\n      PersonServices.getPersons().then(response => {\n        this.personsList.push(response.data);\n      });\n    },\n\n    addPerson: function () {\n      this.personsList.push({\n        name: this.personName,\n        age: this.personAge\n      });\n      this.personName = '';\n    }\n  },\n\n  created() {\n    this.getPersons();\n  }\n\n};\n/*import axios from 'axios'\n\n    export default {\n        name:'PersonsList',\n        data() {\n            return {\n                personsList: [],\n            }\n        },\n        methods: {\n            getPersons(){\n                axios.get('http://localhost:8080/api/v1/persons')\n                    .then((response) => {\n                        this.personsList = response.data\n                        console.log(response.data)\n                    })\n                    .catch((error) => {\n                        console.log(error)\n                    })\n            }\n        },\n        create(){\n            this.getPersons()\n        }\n    }*/","map":{"version":3,"mappings":";AAoCA,OAAOA,cAAP,MAA2B,4BAA3B;AAEI,eAAe;EACXC,IAAI,EAAE,aADK;;EAEXC,IAAI,GAAE;IACF,OAAO;MACHC,WAAU,EAAI,EADX;MAEHC,UAAU,EAAE,EAFT;MAGHC,SAAS,EAAE;IAHR,CAAP;EAOH,CAVU;;EAWXC,OAAO,EAAE;IACLC,UAAU,GAAE;MACRP,cAAc,CAACO,UAAf,GAA4BC,IAA5B,CAAkCC,QAAD,IAAc;QAC3C,KAAKN,WAAL,CAAiBO,IAAjB,CAAsBD,QAAQ,CAACP,IAA/B;MACH,CAFD;IAGH,CALI;;IAMLS,SAAS,EAAE,YAAU;MACjB,KAAKR,WAAL,CAAiBO,IAAjB,CAAsB;QAClBT,IAAI,EAAE,KAAKG,UADO;QAElBQ,GAAG,EAAE,KAAKP;MAFQ,CAAtB;MAIA,KAAKD,UAAL,GAAkB,EAAlB;IACJ;EAZK,CAXE;;EA0BXS,OAAO,GAAG;IACN,KAAKN,UAAL;EACJ;;AA5BW,CAAf;AAgCJ","names":["PersonServices","name","data","personsList","personName","personAge","m