{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PersonServices from '../services/PersonServices';\nexport default {\n  name: 'PersonsList',\n\n  data() {\n    return {\n      personsList: [],\n      // Trie\n      personName: '',\n      personAge: ''\n    };\n  },\n\n  methods: {\n    getPersons() {\n      PersonServices.getPersons().then(response => {\n        this.personsList = response.data;\n      });\n    },\n\n    addPerson: function () {\n      PersonServices.getPersons().then(response => {\n        this.personsList = response.data;\n        this.personsList.push({\n          name: \"James Doe\",\n          age: 33\n        });\n      });\n    }\n  },\n\n  created() {\n    this.getPersons();\n    this.addPerson();\n  }\n\n};\n/*import axios from 'axios'\n\n    export default {\n        name:'PersonsList',\n        data() {\n            return {\n                personsList: [],\n            }\n        },\n        methods: {\n            getPersons(){\n                axios.get('http://localhost:8080/api/v1/persons')\n                    .then((response) => {\n                        this.personsList = response.data\n                        console.log(response.data)\n                    })\n                    .catch((error) => {\n                        console.log(error)\n                    })\n            }\n        },\n        create(){\n            this.getPersons()\n        }\n    }*/","map":{"version":3,"mappings":";AAmCA,OAAOA,cAAP,MAA2B,4BAA3B;AAEI,eAAe;EACXC,IAAI,EAAE,aADK;;EAEXC,IAAI,GAAE;IACF,OAAO;MACHC,WAAU,EAAI,EADX;MAEH;MACAC,UAAU,EAAE,EAHT;MAIHC,SAAS,EAAE;IAJR,CAAP;EAQH,CAXU;;EAYXC,OAAO,EAAE;IACLC,UAAU,GAAE;MACRP,cAAc,CAACO,UAAf,GAA4BC,IAA5B,CAAkCC,QAAD,IAAc;QAC3C,KAAKN,WAAL,GAAmBM,QAAQ,CAACP,IAA5B;MACH,CAFD;IAGH,CALI;;IAMLQ,SAAS,EAAE,YAAU;MACbV,cAAc,CAACO,UAAf,GAA4BC,IAA5B,CAAkCC,QAAD,IAAc;QAC/C,KAAKN,WAAL,GAAmBM,QAAQ,CAACP,IAA5B;QACA,KAAKC,WAAL,CAAiBQ,IAAjB,CAAsB;UACtBV,IAAI,EAAE,WADgB;UAEtBW,GAAG,EAAE;QAFiB,CAAtB;M