Person Name</th>\n                <th>Person Age</th>\n            </thead>\n            <tbody>\n                <tr v-for = \"(person, index) in personsList\" v-bind:key = \"person.name\" @remove = \"removePerson(index)\" >\n                    <td>{{person.name}}</td>\n                    <td>{{person.age}}</td>\n                    <td><button @click=\"$emit('edit')\">Edit</button></td>\n                    <td><button @click=\"$emit('remove')\">Delete</button></td>\n                </tr>\n            </tbody>\n        </table>\n        <div class=\"container\">\n                <form action=\"\">\n                    <div>\n                        <label for=\"name\"></label>\n                        <input v-model=\"personName\" type=\"text\" placeholder=\"Name\" id=\"personName2\">\n                    </div>\n                    <div>\n                        <label for=\"age\"></label>\n                        <input v-model=\"personAge\" type=\"text\" placeholder=\"Age\" id=\"personAge2\">\n                    </div>\n                </form>\n                <button @click=\"addPerson\">Add</button>\n                <p>{{personName}} is {{personAge}} years old</p>\n        </div>\n    </div>\n</template>\n\n<script>\nimport PersonServices from '../services/PersonServices';\n\n    export default {\n        name: 'PersonsList',\n        data(){\n            return {\n                personsList : [],\n                person: {\n                    name:\"\",\n                    age: \"\"\n                }\n            }\n            \n        },\n        methods: {\n            getPersons(){\n                PersonServices.getPersons().then((response) => {\n                    this.personsList = response.data;\n                })\n            }\n        },\n        created() {\n            this.getPersons();\n        }\n    }\n\n\n/*import axios from 'axios'\n\n    export default {\n        name:'PersonsList',\n        data() {\n            return {\n     