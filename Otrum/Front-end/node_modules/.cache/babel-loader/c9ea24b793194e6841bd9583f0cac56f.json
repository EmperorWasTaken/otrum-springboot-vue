turn {\n                personsList : [{name: 'Kai'}, {age: '27'}, {id: '0'}]\n            }\n            \n        },\n        methods: {\n            getPersons(){\n                PersonServices.getAllPersons().then((response) => {\n                    this.personsList = response.data;\n                })\n            }\n        },\n        create() {\n            this.getAllPersons()\n        }\n    }\n</script>\n\n"]},"metadata":{},"sourceType":"module"}